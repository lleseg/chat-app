<%- if controller_name != 'sessions' %>
  <%= link_to "Log in", new_session_path(resource_name), class: 'bg-yellow-800 rounded p-3 text-white font-semibold' %><br />
<% end %>
<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Sign up", new_registration_path(resource_name), class: 'bg-yellow-800 rounded p-3 text-white font-semibold' %><br />
<% end %>
